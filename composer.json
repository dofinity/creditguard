{
    "name": "dofinity/creditguard",
    "type": "library",
    "description": "PHP Library for integrating CreditGuard based payments.",
    "keywords": ["PHP", "payment", "commerce"],
    "homepage": "https://www.dofinity.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7.27",
        "wsdl2phpgenerator/wsdl2phpgenerator": "dev-master"
    },
    "authors": [
        {
            "name": "Rotem Reiss",
            "email": "rotem@dofinity.com",
            "homepage": "https://github.com/dofinity"
        }
    ],
    "autoload": {
        "psr-4": {
            "Creditguard\\": "src/Creditguard"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creditguard\\": "test/Creditguard"
        }
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "phpunit --whitelist src ./test"
    }
}
